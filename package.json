{
  "name": "serverless-graphql-benchmarks",
  "version": "0.0.1",
  "description": "benchmark stuff",
  "main": "src/",
  "config": {
    "jsSrc": "src/"
  },
  "scripts": {
    "test": "ava",
    "watch:test": "ava --watch",
    "lint": "npm run lint:eslint -s",
    "lint:eslint": "eslint $npm_package_config_jsSrc",
    "lint:jscs": "jscs $npm_package_config_jsSrc/",
    "deploy": "npm test && serverless deploy -v",
    "benchmark": "babel-node src/benchmark.js",
    "build:koa": "webpack --config webpack-koa.config.js --progress --profile --colors",
    "koa": "babel-node src/koa.js",
    "express": "babel-node src/express.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adieuadieu/serverless-benchmarks.git"
  },
  "keywords": [
    "graphql",
    "benchmarks",
    "lambda",
    "serverless"
  ],
  "author": "Marco LÃ¼thy",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/adieuadieu/serverless-benchmarks/issues"
  },
  "homepage": "https://github.com/adieuadieu/serverless-benchmarks",
  "dependencies": {
    "express": "^4.14.1",
    "graphql": "^0.9.1",
    "koa": "2.0.0-alpha.7",
    "mathjs": "^3.9.1",
    "progress": "1.1.8",
    "request": "2.79.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-ava": "^4.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-promise": "^3.4.1",
    "json-loader": "0.5.4",
    "serverless": "^1.6.1",
    "serverless-webpack": "^1.0.0-rc.4",
    "webpack": "^2.2.1"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4.3
          }
        }
      ],
      "es2015",
      "es2016",
      "es2017",
      "stage-3"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "ava",
      "import"
    ],
    "extends": [
      "airbnb-base",
      "plugin:ava/recommended"
    ],
    "settings": {
      "import/parser": "babel-eslint",
      "import/resolve": {
        "moduleDirectory": [
          "node_modules",
          "src"
        ]
      }
    },
    "rules": {
      "no-console": 0,
      "max-len": [
        2,
        140,
        2
      ],
      "semi": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  }
}
